<% if (products && products.length) { %>
<table class="table">
  <thead>
    <tr>
      <th scope="col">Nombre</th>
      <th scope="col">Precio</th>
      <th scope="col">Foto</th>
    </tr>
  </thead>
  <tbody>
    <% products.forEach(function(product) { %> <%- include('productRow', {item:
    product}) %> <% }); %>
  </tbody>
</table>
<% } else { %>
<div class="alert alert-dark" role="alert">No hay productos</div>
<% } %>
